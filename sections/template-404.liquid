<h1>{{ section.settings.title }}</h1>
<p>{{ 404 subtext }}</p>

{% schema %}
  {
    "name": "Template 404",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Title",
        "default": "404 Not Found"
      }, {
        "type": "text",
        "id": "subtext",
        "label": "Subtext",
        "default": "The page you are looking for does not exist."
      }
    ]
  }

{% endschema %}